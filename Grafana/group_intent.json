          "rawSql": "SELECT\r\n  snapshot.end as \"time\",\r\n  sum(intent_results.green) as \"green\",\r\n  sum(intent_results.blue) as \"blue\",\r\n  sum(intent_results.amber) as \"amber\",\r\n  sum(intent_results.red) as \"red\"\r\nFROM \"group\"\r\nRIGHT JOIN intent_mapping on \"group\".group_id = intent_mapping.group_id\r\nRIGHT JOIN intent on intent.intent_id = intent_mapping.intent_id\r\nJOIN intent_results on intent.intent_id = intent_results.intent_id\r\njoin snapshot on snapshot.snapshot_id = intent_results.snapshot_id\r\nWHERE \"group\".name = ${group_name}\r\nGROUP BY snapshot.end\r\nORDER BY \"time\"\r\n\r\n",
